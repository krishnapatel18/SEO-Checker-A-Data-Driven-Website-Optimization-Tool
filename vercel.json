{
    "version": 2,
    "builds": [
        {
            "src": "seo_tool/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "seo_tool/wsgi.py"
        }
    ],
    "buildCommand": "./build_files.sh"
}